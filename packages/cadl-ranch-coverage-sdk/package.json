        "url": "https://github.com/Azure/cadl-ranch/issues"
    },
    "homepage": "https://github.com/Azure/cadl-ranch#readme",
    "dependencies": {
        "@azure/identity": "^3.0.0",
        "@azure/storage-blob": "^12.11.0",
        "@types/node": "^18.7.16"
    },
    "devDependencies": {
        "@cadl-lang/eslint-config-cadl": "~0.5.0",
        "eslint": "^8.23.1",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.8.3"
    }
